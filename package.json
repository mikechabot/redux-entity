{
  "name": "redux-entity",
  "version": "7.1.1",
  "description": "A predictable approach to managing domain entities in Redux.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "compile": "babel src -d lib",
    "test": "nyc mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf ./lib",
    "example": "node example.js"
  },
  "browserslist": [
    "last 2 versions",
    "ie 9-11"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "temp-directory": "./.nyc-coverage-temp",
    "report-dir": "./.nyc-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/redux-entity.git"
  },
  "keywords": [
    "redux",
    "redux-thunk",
    "asynchronous"
  ],
  "author": "Mike Chabot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikechabot/redux-entity/issues"
  },
  "homepage": "https://github.com/mikechabot/redux-entity#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "chalk": "^2.4.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "expect": "^23.6.0",
    "jest-mock": "^23.2.0",
    "mocha": "^5.1.1",
    "nyc": "^13.1.0",
    "redux": "^4.0.1",
    "redux-mock-store": "~1.5.3",
    "redux-thunk": "^2.3.0"
  },
  "tonicExampleFilename": "example.js",
  "dependencies": {
    "@babel/register": "^7.0.0"
  }
}
